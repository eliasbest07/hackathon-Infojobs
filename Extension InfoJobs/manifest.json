{
    "manifest_version": 3,
    "name": "Agenda Entrevista Infojobs",
    "version": "1.0",
    "description": "Encuentra tu vacante ideal y agenda una entrevista de 15min",
    "permissions": [
        "notifications", 
        "activeTab",
        "tabs",
        "storage"
      ],
      "host_permissions": [
        "https://api.infojobs.net/"
      ],
    "background": {
        "service_worker": "background.js"
      },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
    },
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }